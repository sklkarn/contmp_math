  <example xml:id="set_exmB1Q1">
    <statement>
      <p>
        Find the union of each of the following pairs of sets:
        <ol>
          <li>
            <p>
              <m>X = \{1, 3, 5\}</m>, <m>Y=\{1, 2, 3\}</m>
            </p>
          </li>
          <li>
            <p>
              <m>A = \{a, e, i, o, u\}</m>, <m>B=\{a, b, c\}</m>
            </p>
          </li>
          <li>
            <p>
              <m>A = \{x : x \text{ is a natural number and multiple of } 3 \}</m>, 
              <m>B=\{x : x \text{ is a natural number less than } 6 \}</m>
            </p>
          </li>
          <li>
            <p>
              <m>A = \{x : x \text{ is a natural number and } 1 \lt x \leq 6 \}</m>, 
              <m>B=\{x : x \text{ is a natural number and } 6 \lt x \lt 10 \}</m>
            </p>
          </li>
          <li>
            <p>
              <m>A = \{1, 2, 3\}</m>, <m>B=\phi</m>.
            </p>
          </li>
        </ol>
      </p>
    </statement>
  </example>
    
